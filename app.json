{
  "expo": {
    "name": "DropVices",
    "slug": "Teste",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/logoDropVices.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/imgs/logoDropVices.png",
      "resizeMode": "contain",
      "backgroundColor": "#73AA9D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/logoDropVices.png",
        "backgroundColor": "#73AA9D"
      },
      "package": "dropvices.com",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "http"
            },
            {
              "scheme": "https"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/imgs/logoDropVices.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bd0a635d-bd03-429e-9517-925f78cd12d8"
      }
    }
  }
}
